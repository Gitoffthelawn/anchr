{
    "manifest_version": 2,
    "name": "Anchr Bookmarks Collector",
    "author": "Ferdinand MÃ¼tsch",
    "homepage_url": "https://github.com/n1try/anchr",
    "version": "0.1.0",
    "description": "Easily bookmark webpages to your Anchr collection",
    "options_ui": {
        "page": "src/options.html",
        "browser_style": true
    },
    "applications": {
        "gecko": {
            "id": "anchr@muetsch.io"
        }
    },
    "icons": {
        "48": "icon/icon.svg",
        "128": "icon/icon.svg"
    },
    "browser_action": {
        "default_icon": "icons/logo-48.png",
        "default_title": "Anchr.io",
        "default_popup": "src/view.html"
    },
    "permissions": [
        "storage"
    ]
}